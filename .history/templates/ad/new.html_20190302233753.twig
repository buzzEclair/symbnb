{% extends 'base.html.twig' %}

{% block title %}Création d'une annone{% endblock %}

{% form_theme form _self %}

{% block body %}
    <div class="container">
        <h1>Création d'une annonce</h1>

        {{ form_start(form)}}

        {{form_widget(form)}}

        

        <button class="btn btn-primary" type="submit">Créer une annonce</button>

        {{ form_end(form) }}
    </div>
{% endblock %}

{% block _ad_images_widget %}
    <p>Ajouter vos propres images</p>
    {{ form_widget(form) }}
{% endblock %}